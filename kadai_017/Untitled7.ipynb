        "class Human:\n",
        "  def __init__(self, name, age):\n",
        "   self.name = name\n",
        "   self.age = age\n",
        "\n",
        "  def check_adult(self):\n",
        "   if self.age >= 20:\n",
        "    print(f\"{self.name}さんは大人です。\")\n",
        "   else:\n",
        "    print(f\"{self.name}さんは大人ではありません。\")\n",
        "\n",
        "human = Human(\"侍太郎\", 19)\n",
        "\n",
        "human.check_adult()"
      ]
    }
  ]
